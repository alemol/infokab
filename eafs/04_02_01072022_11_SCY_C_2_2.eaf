    <TIER ANNOTATOR="FCA" LANG_REF="spa"
        LINGUISTIC_TYPE_REF="Transcripción Ortográfico"
        PARENT_REF="tx@JBT" PARTICIPANT="JBT" TIER_ID="to@JBT">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a2" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>- In kaajala' u k'aabae San Silverio,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a13" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>- In kuxtale' pos chen yeetel ichkool</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a16" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>- Kin biino'on ichkool, kin biino'on tak si', kin kóon si'on,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a25" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>- Chen a'ajko'on beyo' pos in mamaa leti'e binkaj ju'uch' k'eyeen, de' echoo in mama' leti'e jach maas teemprano' ku ya'ajal, leti'e k'anáan u liisto'kunsik le k'eymo', men chen li'ikin papaj leti'e, liisto' e k'eymo' chen ken u' ch'ae, to'one tin ch'iiko'on, tin chuupik in mejen chujo'on tin luuk'lo'on tun, jach taan u píisa'astale' tin k'uuchlo'on tin ichkoolo'on, chen k'uuchko'on pos binkaj p'íisbi in mejen meyajo'on, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>- In papaj uchile', syempree ku ts'iikto'on jun k'aan kool, jun k'aan páak, e tiiempo tun jéelo' to'one pos páak, ma' jach k'uuchuk ik muuk' ti' le' koolo', peroo ku toocarxan in meyajtko'oni', ka' tuulo'on yéetel juntul in suukun u k'aabae Roman, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>- Chen xiiko'on tun, koomo' yaax to'on bey nunukuch paalalo'ono' pos to'on kiin biislo'on ichkool, chen k'uuchko'on tune to'one binkajo'on páak, yaan óora tun in papaj tun leti'e koomo' seeb u meyaj leti' tun yaan óorae' ku, ku, ku ts'ooksike ti' tuno', ku, ku yaantko'on, in wa'ake chen ya' las 12 wa' las 10, wa bixe' tun ya'awatko'on uk'u' k'eyeen, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
